; Zero length search
; Expect `data` as return value
 set a, data
 set b, 3
 set c, 0
 jsr memchr

 set b, data
 jsr asserteq

; Non existing value
; Expect 0 (NULL) as return value.
 set a, data
 set b, 10
 set c, 5
 jsr memchr

 set b, 0
 jsr asserteq

; Existing value.
; Expect data+2 as return value.
 set a, data
 set b, 3
 set c, 5
 jsr memchr

 set b, data
 add b, 2
 jsr asserteq

 exit

:data
 dat 1, 2, 3, 4, 5
